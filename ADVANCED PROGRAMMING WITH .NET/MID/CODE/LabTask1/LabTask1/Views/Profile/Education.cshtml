
@{
    ViewBag.Title = "Education";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}

<h2>Education</h2>
<table class="table table-striped table-bordered table-hover">
    
    <thead class="thead-dark">
    <tr class="text-center">
        <th>Degree</th>
        <th>Result</th>
        <th>Year</th>
    </tr>
    </thead>
    @foreach(var item in Model)
       {
                <tr class="text-center">
                    <td>@item.degree</td>
                    <td>@item.result</td>
                    <td>@item.year</td>
                </tr>
       }
</table>

