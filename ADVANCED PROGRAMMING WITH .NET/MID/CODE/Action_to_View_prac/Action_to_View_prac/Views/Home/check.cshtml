
@{
    ViewBag.Title = "check";
}

<h2>check</h2>
<h1>Hello, @ViewBag.Name</h1> 
<h2>@ViewBag.Name 's cgpa is @ViewData["Cgpa"]</h2> 
<h3>@ViewBag.Name 's ID is @TempData["Id"]</h3>




<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>CGPA</th>
    </tr>
@foreach(var item in Model)
            {
                <tr class="text-center">
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Cgpa</td>
                </tr>
            }
</table>
